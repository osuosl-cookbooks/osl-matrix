id: <%= @id %>
url: <%= @url %>
as_token: <%= @matrix_rand_appservice %>
hs_token: <%= @matrix_rand_homeserver %>
<% if @limit %>
rate_limited: <%= @limit %>
<% else %>
rate_limited: false
<% end %>
sender_localpart: <%= @service %>
<% if @namespaces %>
namespaces:
<% @namespaces.each do |ns_key, ns_val| -%>
  <%= ns_key -%>:
  <% ns_val.each do |regex_group| -%>
    -
      <%- regex_group.each do |regex_key, regex_val| -%>
      <%= regex_key %>: <%= regex_val %>
    <%- end -%>
  <% end %>
<% end %>
<% end %>
